{
  "name": "prettier-pnp",
  "version": "0.2.0",
  "packageManager": "pnpm@8.7.4",
  "author": "auvred <aauvred@gmail.com> (https://github.com/auvred/)",
  "type": "module",
  "description": "Run Prettier with plugins without installing them manually",
  "keywords": [
    "format",
    "prettier"
  ],
  "homepage": "https://github.com/auvred/prettier-pnp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/auvred/prettier-pnp"
  },
  "license": "MIT",
  "bin": "dist/bin.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "release": "bumpp",
    "test": "pnpm build && vitest",
    "type-check": "tsc --noEmit",
    "type-check-test": "tsc --noEmit -p tsconfig.test.json"
  },
  "dependencies": {
    "import-meta-resolve": "^3.0.0",
    "prettier": "^3.0.3"
  },
  "devDependencies": {
    "@auvred/eslint-config": "^0.4.0",
    "@types/node": "^18.17.14",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "tsup": "^7.2.0",
    "typescript": "~5.1.6",
    "vitest": "^0.34.3"
  }
}
