  },
  "devDependencies": {
    "@auvred/eslint-config": "^0.4.0",
    "@types/node": "^20.4.8",
    "eslint": "^8.46.0",
    "tsup": "^7.2.0",
    "typescript": "~5.1.6",
    "vitest": "^0.34.1"
  }
}
